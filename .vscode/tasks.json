{
    "type": "shell",
    "label": "g++ build active file",
    "command": "g++",
    "args": [
        "-g",
        "${file}",
        "-I${workspaceFolder}/C-Cpp/include/cpp-httplib-master",
        "-I${workspaceFolder}/C-Cpp/include/json-develop/include",
        "-I$C:/Program Files/OpenSSL-Win64/include",
        "-L$C:/Program Files/OpenSSL-Win64/lib",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}.exe",
        "-lssl",
        "-lcrypto",
        "-lws2_32",
        "-lwsock32",
        "-lcrypt32"
    ],
    "options": {
        "cwd": "${workspaceFolder}"
    },
    "problemMatcher": [
        "$gcc"
    ],
    "group": {
        "kind": "build",
        "isDefault": true
    },
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe build active file",
            "command": "C:/msys64/mingw64/bin/g++.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe"
            ],
            "options": {
                "cwd": "C:/msys64/mingw64/bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "${file}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}