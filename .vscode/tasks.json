{
    "type": "shell",
    "label": "g++ build active file",
    "command": "g++",
    "args": [
        "-g",
        "${file}",
        "-I${workspaceFolder}/C-Cpp/include/cpp-httplib-master",
        "-I${workspaceFolder}/C-Cpp/include/json-develop/include",
        "-I$C:/Program Files/OpenSSL-Win64/include",
        "-L$C:/Program Files/OpenSSL-Win64/lib",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}.exe",
        "-lssl",
        "-lcrypto",
        "-lws2_32",
        "-lwsock32",
        "-lcrypt32"
    ],
    "options": {
        "cwd": "${workspaceFolder}"
    },
    "problemMatcher": [
        "$gcc"
    ],
    "group": {
        "kind": "build",
        "isDefault": true
    }
}